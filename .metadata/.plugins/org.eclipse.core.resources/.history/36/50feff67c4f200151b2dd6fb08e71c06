import com.google.api.GoogleAPI;
import com.google.api.GoogleAPIException;
import com.google.api.translate.Language;
import com.google.api.translate.Translate;


public class Test {
	public static void main(String[] args)throws GoogleAPIException{
		
		GoogleAPI.setHttpReferrer("https://code.google.com/archive/p/google-api-translate-java/");
		GoogleAPI.setKey("AIzaSyA3cSo93U0WWWXM0h798GPdfhAsL19F3Gw");
		String text = Translate.DEFAULT.execute("manger", Language.FRENCH, Language.ENGLISH);
		System.out.println(text);
	}
}
